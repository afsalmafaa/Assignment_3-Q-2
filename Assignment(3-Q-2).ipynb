    "def reverse_string(str):\n",
    "    rstr =''\n",
    "    index = len(str)\n",
    "    while index>0:\n",
    "        rstr +=str[index - 1]\n",
    "        index = index - 1\n",
    "    return rstr\n",
    "sample_string = \"1234abcd\"\n",
    "print(\"Sample String:\",sample_string)\n",
    "print(\"Expected Output:\",reverse_string(sample_string))"
   ]
